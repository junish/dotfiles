" Options {{{
set guioptions=bCrsBn
set hintkeys='asdfjlk;'
" }}}

" Mappings {{{
nnoremap j 10<C-e>
nnoremap k 10<C-y>
nnoremap J h
nnoremap K l
nnoremap h H
nnoremap l L
nnoremap <silent> H :tabprevious<CR>
nnoremap <silent> L :tabnext<CR>
nnoremap <silent> <C-S-h> :tabmove! -1<CR>
nnoremap <silent> <C-S-l> :tabmove! +1<CR>
nnoremap <silent> x :tabclose<CR>

nnoremap <silent> a :set guioptions+=mT<CR>
nnoremap <silent> A :set guioptions-=mT<CR>
" }}}

" PassKeys {{{
set passkeys+="https://mail\.google\.com/":jkg
set passkeys+="https://www\.google\.com/calendar":jkg12345qrcnp
set passkeys+="https://twitter\.com/":jkg/?.nrm
" }}}
