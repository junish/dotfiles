" Options {{{
set guioptions=bCrsBn
set hintkeys='asdfjlk;'
" }}}

" Mappings {{{
nmap -builtin j 5j
nmap -builtin k 5k
nmap -builtin J h
nmap -builtin K l
nmap -builtin h H
nmap -builtin l L
nmap -builtin <C-j> <C-d>
nmap -builtin <C-k> <C-u>
nmap -builtin <silent> <C-h> :tabprevious<CR>
nmap -builtin <silent> <C-l> :tabnext<CR>
nmap -builtin <silent> H :tabmove! -1<CR>
nmap -builtin <silent> L :tabmove! +1<CR>
nmap -builtin <silent> x :tabclose<CR>

nmap -builtin <silent> a :set guioptions+=mT<CR>
nmap -builtin <silent> A :set guioptions-=mT<CR>
" }}}

" PassKeys {{{
set passkeys+="https://mail\.google\.com/":jkg
set passkeys+="https://www\.google\.com/calendar":jkg12345qrcnp
set passkeys+="https://twitter\.com/":jkg/?.nrm
" }}}
