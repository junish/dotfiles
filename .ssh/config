Host *
    ServerAliveInterval 60
    ControlMaster yes

Host *.local.hde.co.jp
    ProxyCommand ssh -l shinohara -p 6357 -o ClearAllForwardings=yes pix.hde.co.jp /home/shinohara/bin/netcat %h %p
    
Host pix.hde.co.jp
    HostName pix.hde.co.jp
    Port 6357
    User shinohara
    LocalForward 9080 proxy.prd.local.hde.co.jp:9080
    LocalForward 127.0.0.2:8139 kabenuri2007.local.hde.co.jp:139
    LocalForward 127.0.0.2:8445 kabenuri2007.local.hde.co.jp:445
